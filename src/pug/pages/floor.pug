					//end tabs-floor__item
				.tabs-floor__content
					.tabs-floor__content-in.is-active.js-list
						//begin tabs-floor-item
						.floor__wrap
							.floor__left
								.floor__row
									+svg('star').rooms1
									span 1 кімнатні
								.floor__row
									+svg('star').rooms2
									span 2 кімнатні
								.floor__row
									+svg('star').rooms3
									span 3 кімнатні
								.floor__row
									+svg('star').rooms4
									span 4 кімнатні
							.floor__right
								.floor__img.js-choise-apartment-img
