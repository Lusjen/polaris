extends ../layout/main
block variable
	-
		var titlePage = 'План поверху';
		var crumbs = [
			{
				title: 'Головна',
				link: pagesLink[0].home
			},
			{
				title: 'Обрати поверх',
				link: pagesLink[0].chooseFloor
			}
		];
	
block meta
	title= 'Home'
	meta(name='description', content='')
block head
		link(rel='stylesheet', href="./assets/styles/main.min.css")
block scripts
	script(src='./assets/scripts/index.js')
	script(src='./assets/scripts/tabs.js')
	script(src='./assets/scripts/floor.js')


block content
	.mini-title.mini-title_white.container
		include ../includes/crumbs.pug
		.heading
			.heading__left
				+svg('line-title')
				h1.page-title План поверху
			.heading__right
				a(href="./filter.html").heading__link
					+svg('filter')
					span Обрати за параметрами
				.heading__img
					img(src="./assets/images/compas.svg")

	.container
		.floor
			.floor__tabs.tabs-floor
				.tabs-floor__header
					//begin date__item
					.tabs-floor__item.is-active.js-btn(data-year="0")
						.tabs-floor__name Секція 1
					//end tabs-floor__item
					//begin tabs-floor__item
					.tabs-floor__item.js-btn(data-year="1")
						.tabs-floor__name Секція 2
